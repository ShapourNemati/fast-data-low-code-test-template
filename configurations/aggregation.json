{
    "version": "1.1.0",
    "config": {
      "SV_CONFIG": {
        "dependencies": {
          "USER": {
            "type": "projection",
            "on": "_identifier"
          },
          "JOB_DESCRIPTION": {
            "type": "projection",
            "_select": {
              "options": [
                {
                    "when": {
                        "==": ["USER.job", "__string__[doctor]"]
                    },
                    "value": {
                        "type": "projection",
                        "aliasOf": "DOCTORS",
                        "on": "U_to_D"
                    }
                  },
                  {
                    "when": {
                        "==": ["USER.job", "__string__[firefighter]"]
                    },
                    "value": {
                        "type": "projection",
                        "aliasOf": "FIREFIGHTERS",
                        "on": "U_to_F"
                    }
                }
              ],
              "default": {
                "type": "projection",
                "aliasOf": "DOCTORS",
                "on": "U_to_D"
              }
            }
          }
        },
        "mapping": {
            "name": "USER.name",
            "starting_date": "JOB_DESCRIPTION.startedAt"
        }
      }
    }
  }